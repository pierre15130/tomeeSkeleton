<faces-config xmlns="http://java.sun.com/xml/ns/javaee"	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"	version="2.0">		<navigation-rule>		<display-name>login/login.xhtml</display-name>		<from-view-id>/login/login.xhtml</from-view-id>		<navigation-case>			<from-outcome>loginSuccess</from-outcome>			<to-view-id>/app/home.xhtml</to-view-id>			<redirect />		</navigation-case>	</navigation-rule>		<navigation-rule>		<display-name>login/login.xhtml</display-name>		<from-view-id>/login/login.xhtml</from-view-id>		<navigation-case>			<from-outcome>loginFailed</from-outcome>			<to-view-id>/login/login.xhtml</to-view-id>		</navigation-case>	</navigation-rule>		<navigation-rule>		<display-name>/app/home.xhtml</display-name>		<from-view-id>/app/home.xhtml</from-view-id>		<navigation-case>			<from-outcome>disconnect</from-outcome>			<to-view-id>/login/login.xhtml</to-view-id>		</navigation-case>	</navigation-rule>		<navigation-rule>		<display-name>/app/home.xhtml</display-name>		<from-view-id>/app/home.xhtml</from-view-id>		<navigation-case>			<from-outcome>composeTweet</from-outcome>			<to-view-id>/app/composeTweet.xhtml</to-view-id>		</navigation-case>	</navigation-rule>		<navigation-rule>		<display-name>/app/composeTweet.xhtml</display-name>		<from-view-id>/app/composeTweet.xhtml</from-view-id>		<navigation-case>			<from-outcome>tweetSuccess</from-outcome>			<to-view-id>/app/home.xhtml</to-view-id>		</navigation-case>	</navigation-rule>		<navigation-rule>		<display-name>/app/home.xhtml</display-name>		<from-view-id>/app/home.xhtml</from-view-id>		<navigation-case>			<from-outcome>connect</from-outcome>			<to-view-id>/app/connect.xhtml</to-view-id>		</navigation-case>	</navigation-rule>	
</faces-config>